{
  "name": "Corporater Dark",
  "type": "dark",
  "colors": {
    "editor.background": "#1E1E1E",
    "editor.foreground": "#D4D4D4",
    "activityBar.background": "#2D2D30",
    "sideBar.background": "#252526"
  },
  "tokenColors": [
    {
      "name": "Control Keywords (IF ELSE)",
      "scope": ["keyword.control"],
      "settings": { "foreground": "#4FC1FF", "fontStyle": "bold" }
    },
    {
      "name": "Context Keywords (this, self, after)",
      "scope": ["variable.language.context.extended"],
      "settings": { "foreground": "#569CD6", "fontStyle": "italic" }
    },
    {
      "name": "Brackets & Punctuation (Default Red)",
      "scope": ["punctuation.section.brackets.extended"],
      "settings": { "foreground": "#ff0039", "fontStyle": "bold" }
    },
    {
      "name": "Action Runner Brackets ({}) - Green",
      "scope": ["punctuation.section.brackets.runner.extended"],
      "settings": { "foreground": "#4CAF50", "fontStyle": "bold" }
    },
    {
      "name": "Object Types (Kpi, Incident...)",
      "scope": ["entity.name.class.extended"],
      "settings": { "foreground": "#008080", "fontStyle": "bold" }
    },
    {
      "name": "ID Namespaces (t., o., ceven.)",
      "scope": ["entity.name.type.namespace.extended"],
      "settings": { "foreground": "#569CD6" }
    },
    {
      "name": "Double Quotes (Soft Orange)",
      "scope": ["string.quoted.double"],
      "settings": { "foreground": "#CE9178" }
    },
    {
      "name": "Single Quotes (Amber)",
      "scope": ["string.quoted.single"],
      "settings": { "foreground": "#FFCA28" }
    },
    {
      "name": "Variable Interpolation ${...}",
      "scope": ["variable.other.interpolation.extended"],
      "settings": { "foreground": "#4FC1FF", "fontStyle": "bold" }
    },
    {
      "name": "Numbers (Soft Purple)",
      "scope": ["constant.numeric"],
      "settings": { "foreground": "#D197D9" }
    },
    {
      "name": "Action Runners (Green = GO)",
      "scope": ["entity.name.function.runner.extended"],
      "settings": { "foreground": "#4CAF50", "fontStyle": "bold" }
    },
    {
      "name": "Commands/Methods (Functional)",
      "scope": [
        "support.function.method.extended", 
        "support.function.global.extended", 
        "support.function.table.extended",
        "entity.name.function.call.extended"
      ],
      "settings": { "foreground": "#FFCA28", "fontStyle": "italic" }
    },
    {
      "name": "Transactional Methods (DANGER)",
      "scope": ["storage.type.method.transactional.extended"],
      "settings": { "foreground": "#ff0039", "fontStyle": "bold" }
    },
    {
      "name": "Comment (Sage Green)",
      "scope": ["comment"],
      "settings": { "foreground": "#608b4e" }
    },
    {
      "name": "Documentation Tags",
      "scope": ["keyword.other.documentation.extended"],
      "settings": { "foreground": "#608b4e", "fontStyle": "bold" }
    },
    {
      "name": "Model Roots",
      "scope": ["entity.name.class.root.extended"],
      "settings": { "foreground": "#4EC9B0", "fontStyle": "italic" }
    },
    {
      "name": "Custom Architect Variables (_prefix)",
      "scope": ["variable.other.custom.underscore.extended"],
      "settings": { "foreground": "#DCDCAA" }
    },
    {
      "name": "Configuration Keys (Architect)",
      "scope": ["variable.parameter.configuration.extended"],
      "settings": { "foreground": "#D7BA7D" }
    },
    {
      "name": "Custom Extended Variables (vPrefix)",
      "scope": ["variable.other.custom.vprefix.extended"],
      "settings": { "foreground": "#9CDCFE" }
    },
    {
      "name": "System Properties",
      "scope": ["variable.other.property.system.extended"],
      "settings": { "foreground": "#9CDCFE", "fontStyle": "bold" }
    },
    {
      "name": "Illegal / Error",
      "scope": ["invalid.illegal"],
      "settings": { "foreground": "#F44336", "fontStyle": "underline" }
    },
    {
      "name": "Constants (MISSING, TRUE)",
      "scope": ["constant.language", "constant.other.style"],
      "settings": { "foreground": "#AE81FF", "fontStyle": "bold" }
    }
  ]
}