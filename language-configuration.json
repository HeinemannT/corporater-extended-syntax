{
  "comments": {
    "lineComment": "//",
    "blockComment": [ "/*", "*/" ]
  },
  "brackets": [
    [ "{", "}" ],
    [ "[", "]" ],
    [ "(", ")" ]
  ],
  "autoClosingPairs": [
    { "open": "{", "close": "}" },
    { "open": "[", "close": "]" },
    { "open": "(", "close": ")" },
    { "open": "\"", "close": "\"", "notIn": [ "string" ] },
    { "open": "'", "close": "'", "notIn": [ "string" ] }
  ],
  "surroundingPairs": [
    [ "{", "}" ],
    [ "[", "]" ],
    [ "(", ")" ],
    [ "\"", "\"" ],
    [ "'", "'" ]
  ],
  "indentationRules": {
    "increaseIndentPattern": "^\\s*(IF|ELSE|.*\\b(forEach|map|filter)\\s*\\().*$",
    "decreaseIndentPattern": "^\\s*(ELSE|ENDIF|\\)|\\}).*$"
  },
  "onEnterRules": [
    {
      "beforeText": "^\\s*.*\\($",
      "afterText": "^\\s*\\)$",
      "action": {
        "indentAction": "indentOutdent"
      }
    }
  ],
  "wordPattern": "(-?\\d*\\.\\d\\w*)|([^\\`\\~\\!\\@\\#\\%\\^\\&\\*\\(\\)\\-\\=\\+\\[\\{\\]\\}\\\\\\|\\;\\:\\'\\\"\\,\\.\\<\\>\\/\\?\\s]+)"
}